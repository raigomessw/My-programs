package com.Skola;


import javax.swing.*;

public class Main {

    public static void main(String[] args) {

        String s0 = "xJava";
        String s1 = "Java";
        String s2 = s0.substring(1) ;

        if (s1 == s2) {
            JOptionPane.showMessageDialog(null,
                    s1 + "och" + s2 + "är lika" );
        }
        else
            JOptionPane.showMessageDialog(null, s1 + " och " + s2 + " är olika");


    }
}
