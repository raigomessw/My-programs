package com.Hacker;

import javax.swing.*;

public class PersonDemo {

    public static void main(String[] args) {
        Person p =new Person();
        p.förnamn ="Rai";
        p.efternamn="Gomes";
        p.föddÅr=1995;
        String s = p.förnamn + ", född år "+ p.föddÅr +", är";
        if (!p.singel)
            s =s + " inte ";
        s =s + "Singel ";
        JOptionPane.showMessageDialog(null, s);
        }

    }
    class Person  {
    String förnamn;
    String efternamn;
    int föddÅr;
    boolean singel =true;
    }
