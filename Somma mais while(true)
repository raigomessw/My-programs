package com.Skola;


import javax.swing.*;

public class Main {

    public static void main(String[] args) {
        int k = 0;
        String s = "k:";
        while (true)    {
            s = s + "  " + k;
            if (k >= 6)
                break;
            k = k + 2;
        }
        JOptionPane.showMessageDialog(null, s);
    }
}
