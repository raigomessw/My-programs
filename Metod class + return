package com.Hacker;

import javax.swing.*;

public class Main {

    public static void main(String[] args) {
	String s;
	s = JOptionPane.showInputDialog("Det fösta talet?");
	double x = Double.parseDouble(s);
	s = JOptionPane.showInputDialog("Det andra talet?");
	double y = Double.parseDouble(s);
	double mv = medelv (x, y); //Arop av metod
        JOptionPane.showMessageDialog(null, "Medelvärdet:" + mv);

    }
    public static double medelv (double a, double b) {
        return (a+b) /2;
    }
}
